// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id       Int    @id @default(autoincrement())
  name     String
  email    String @unique
  password String
  fusoh String
  horarioverao String
  infoeyesup String
  infoparc String
}

model Status {
  id      Int    @id @default(autoincrement())
  name    String
  address String @unique
  protocolo String
 
}